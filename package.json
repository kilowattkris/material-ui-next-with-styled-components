{
  "name": "yoda",
  "version": "0.16.4",
  "description": "The front end of ELL's new LMS",
  "repository": {
    "type": "git",
    "url": "https://github.com/callemall/material-ui.git"
  },
  "scripts": {
    "startMsg": "babel-node buildScripts/startMessage.js",
    "flow": "flow",
    "flow:run": "yarn run flow",
    "prestart": "npm-run-all startMsg flow:run",
    "start": "npm-run-all --parallel test:watch server lint:watch",
    "server": "babel-node buildScripts/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src buildScripts",
    "lint:watch": "yarn run lint -- --watch",
    "test": "mocha --reporter spec buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "yarn run test -- --watch",
    "prebuild": "npm-run-all dist:clean lib:clean lint test",
    "build": "npm-run-all dist:build lib:build lib:flow",
    "postbuild": "babel-node buildScripts/distServer.js",
    "lib:build": "babel -d lib src --ignore '**/__tests__/**'",
    "lib:flow": "flow-copy-source -v -i '**/__tests__/**' src lib",
    "lib:clean": "rimraf lib",
    "dist:clean": "rimraf ./dist && mkdir dist",
    "dist:build": "babel-node buildScripts/build.js"
  },
  "private": true,
  "dependencies": {
    "babel-polyfill": "^6.18.0",
    "jss": "^6.0.0",
    "material-ui": "^1.0.0-alpha.16",
    "material-ui-icons": "^1.0.0-alpha.3",
    "polished": "^1.1.3",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-event-listener": "^0.4.5",
    "react-jss": "^6.1.1",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.4",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "styled-jss": "^1.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-runtime": "^6.23.0",
    "caniuse-api": "^2.0.0",
    "chalk": "1.1.3",
    "compression": "^1.6.1",
    "cross-env": "^5.0.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-watch": "^3.1.0",
    "eventsource-polyfill": "0.9.6",
    "expect": "^1.19.0",
    "express": "^4.13.4",
    "fbjs": "^0.8.12",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.47.0",
    "flow-copy-source": "^1.1.0",
    "flow-typed": "^2.0.0",
    "history": "^4.6.1",
    "html-webpack-plugin": "^2.24.1",
    "jsdom": "8.5.0",
    "mocha": "^3.4.2",
    "npm-run-all": "^4.0.2",
    "open": "0.0.5",
    "postcss": "^6.0.1",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^2.11.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.5",
    "postcss-reporter": "^3.0.0",
    "postcss-url": "^6.0.1",
    "prop-types": "15.5.10",
    "rimraf": "2.5.2",
    "style-loader": "^0.13.1",
    "styled-components": "^2.0.0",
    "transfer-webpack-plugin": "^0.1.4",
    "url-loader": "^0.5.7",
    "warning": "^3.0.0",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "1.6.1",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "2.10.0",
    "yarn-run-all": "3.1.1"
  }
}
